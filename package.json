{
  "name": "clone-tabnews.com.br",
  "version": "1.0.0",
  "description": "Projeto de clone da plataforma tabnews para o curso.dev",
  "homepage": "https://github.com/salvadorludovico/clone-tabnews.com.br#readme",
  "bugs": {
    "url": "https://github.com/salvadorludovico/clone-tabnews.com.br/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/salvadorludovico/clone-tabnews.com.br.git"
  },
  "license": "MIT",
  "author": "",
  "main": "index.js",
  "scripts": {
    "dev": "npm run services:up && next dev",
    "services:up": "docker compose --file=infra/compose.yaml up -d",
    "services:stop": "docker compose --file=infra/compose.yaml stop",
    "services:down": "docker compose --file=infra/compose.yaml down",
    "lint:check": "prettier --check .",
    "lint:fix": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "dependencies": {
    "next": "^13.1.6",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "jest": "^29.6.2",
    "prettier": "^3.8.1"
  }
}
